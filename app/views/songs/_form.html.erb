<%= form_for @song  do |f| %>
<%binding.pry%>
  <% if @song.artist_id.nil? %>

  <%= artist_select(:artist_id, f) %>
  <% else %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <% end %>
  <br />

  <%= f.submit %>
<% end %>
